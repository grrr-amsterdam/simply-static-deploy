{
  "name": "grrr-amsterdam/simply-static-deploy",
  "description": "Deploy static WordPress sites easily to an AWS S3 bucket",
  "keywords": [
    "wordpress",
    "static site",
    "static site generator",
    "aws",
    "s3",
    "cloudfront"
  ],
  "type": "wordpress-plugin",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/grrr-amsterdam/simply-static-deploy/",
  "authors": [
    {
      "name": "Koen Schaft",
      "email": "koen@grrr.nl"
    },
    {
      "name": "Harmen Janssen",
      "email": "harmen@grrr.nl"
    },
    {
      "name": "Ramiro Hammen",
      "email": "ramiro@grrr.nl"
    }
  ],
  "config": {
    "platform": {
      "php": "7.1"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "prefer-stable": true,
  "require": {
    "php": ">=7.1",
    "aws/aws-sdk-php": "^3.87",
    "grrr-amsterdam/garp-functional": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Grrr\\": "includes/Grrr"
    }
  }
}
